{
  "name": "my-new-project",
  "type": "module",
  "devDependencies": {
    "@cucumber/cucumber-expressions": "^17.1.0",
    "@wdio/allure-reporter": "^9.1.3",
    "@wdio/cli": "^9.2.1",
    "@wdio/cucumber-framework": "^9.1.3",
    "@wdio/local-runner": "^9.2.1",
    "@wdio/mocha-framework": "^9.1.3",
    "@wdio/spec-reporter": "^9.1.3",
    "@wdio/visual-service": "^6.2.1",
    "concurrently": "^9.0.1"
  },
  "scripts": {
    "visual": "CUCUMBER_STEP_REPORTER='false' DISABLE_WEBDRIVER_SCREENSHOTS_REPORTING='true' wdio wdio.conf.js --spec ./test/specs/**/*.ts --framework mocha",
    "e2e": "CUCUMBER_STEP_REPORTER='true' DISABLE_WEBDRIVER_SCREENSHOTS_REPORTING='false' wdio wdio.conf.js --spec ./test/features/**/*.feature",
    "wdio": "concurrently \"npm run visual\" \"npm run e2e\"",
    "allure": "allure generate allure-results/ --clean && allure open"
  }
}
